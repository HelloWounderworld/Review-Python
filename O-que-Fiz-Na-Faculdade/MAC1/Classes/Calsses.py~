class Complexo:
    def __init--(self,real,imaginario):
        self.real = real
        self.imaginario = imaginario

    def __str__(self):
        return "{}{:+}i".format(self.real,self.imaginario)

    def soma(sel,outro):
        return Complexo(self.real+outro.real,self.imagem+outro.imagem)

